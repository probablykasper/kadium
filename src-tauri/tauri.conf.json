{
  "build": {
    "distDir": "../dist",
    "devPath": "http://localhost:9000",
    "beforeDevCommand": "npm run dev:web",
    "beforeBuildCommand": "npm run build:web"
  },
  "package": {
    "productName": "Kadium"
  },
  "tauri": {
    "bundle": {
      "active": true,
      "targets": ["dmg", "deb", "appimage", "msi"],
      "identifier": "space.kasper.kadium",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "resources": [],
      "externalBin": [],
      "copyright": "Â© 2021 kasper.space",
      "category": "Utility",
      "shortDescription": "App for YouTube upload notifications",
      "longDescription": "App for YouTube upload notifications",
      "deb": {
        "depends": []
      },
      "macOS": {
        "frameworks": [],
        "minimumSystemVersion": "10.13",
        "exceptionDomain": "",
        "signingIdentity": null,
        "entitlements": null
      },
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "updater": {
      "active": false
    },
    "allowlist": {
      "dialog": {
        "message": true
      },
      "notification": {
        "all": true
      },
      "shell": {
        "open": true
      }
    },
    "security": {
      "csp": "default-src 'self'; img-src *; style-src 'unsafe-inline' *"
    }
  }
}
