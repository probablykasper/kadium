{
  "build": {
    "distDir": "../build",
    "devPath": "http://localhost:9000",
    "beforeDevCommand": "npm run dev:web",
    "beforeBuildCommand": "npm run build:web"
  },
  "package": {
    "productName": "YouTube Email Notifier"
  },
  "tauri": {
    "systemTray": {
      "iconPath": "./icons/Menu bar icon.png",
      "iconAsTemplate": true
    },
    "bundle": {
      "active": true,
      "targets": [
        "dmg",
        "deb",
        "appimage",
        "msi"
      ],
      "identifier": "space.kasper.youtube-email-notifier",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "resources": [],
      "externalBin": [],
      "copyright": "Â© 2021 kasper.space",
      "category": "Utility",
      "shortDescription": "App for YouTube upload email notifications",
      "longDescription": "App for YouTube upload email notifications",
      "deb": {
        "depends": [],
        "useBootstrapper": false
      },
      "macOS": {
        "frameworks": [],
        "minimumSystemVersion": "",
        "useBootstrapper": false,
        "exceptionDomain": "",
        "signingIdentity": null,
        "entitlements": null
      },
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "updater": {
      "active": false
    },
    "allowlist": {
      "shell": {
        "open": true
      }
    },
    "security": {
      "csp": "default-src blob: data: filesystem: ws: wss: http: https: tauri: 'unsafe-eval' 'unsafe-inline' 'self' img-src: 'self'"
    }
  }
}
